start {

    arg 0 -> N;
    1 -> c;
    false -> printed;
    repeat N times {
            
        false -> printed;
        if ((c % 3) = 0) {
            pr "fizz, ";
            true -> printed;
        } elseif ((c % 5) = 0) {
            pr "buzz, ";
            true -> printed;
        }
        if (printed) {
            pr c;
            pr ", ";
        }
        c + 1 -> c;
    }
    
    prn " ";
    
}
